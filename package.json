{
  "name": "reactstudyproject",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "all": "concurrently \"npm run start\" \"npm run server\" \"npm run storybook\"",
    "test": "npm run lint:scss:fix && npm run lint:ts:fix && npm run test:unit",
    "start": "webpack serve --env port=3000",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "lint:ts": "eslint --config ./config/eslint/eslint.config.js \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint --config ./config/eslint/eslint.config.js \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint --config ./config/eslint/.stylelintrc.json \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint --config ./config/eslint/.stylelintrc.json \"**/*.scss\" --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "test:storybook": "npx loki --requireReference --reactUrl file:./storybook-static --chromeFlags=\"--headless --disable-gpu --hide-scrollbars --no-sandbox\"",
    "storybook": "start-storybook -p 6006 -c ./config/storybook/",
    "storybook:build": "build-storybook -c ./config/storybook/",
    "server": "node ./server/index.js",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@feature-sliced/eslint-config": "^0.1.0-beta.6",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-i18next-extract": "^0.9.0",
